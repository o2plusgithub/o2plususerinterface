    function canvas_container() {
        var v = document.querySelector('video');
        var canvas = document.querySelector('canvas');
        var context = canvas.getContext('2d');


        // if quality is bad, change this value
        canvas.width = 3040; // mobile value
        canvas.height = 3040; // mobilevalue
        //canvas.width = window.innerWidth;  // computer value    // equals window dimension // dpr is newly added
        //canvas.height = window.innerHeight; // computer value      
        cw = canvas.width;
        ch = canvas.height;


        v.addEventListener('play', function() {
            draw(this, context, cw, ch);
        }, false);
    }




    function draw(v, c, w, h) {
        if (v.paused || v.ended) return false;
        c.drawImage(v, 0, 0, w, h);
        //c.fillStyle = "RGB(255, 255, 255)";
        //c.fillRect(30,100, 100, 100 );
        //c.font = "30px monospace";
        //c.fillStyle = 'black';
        //c.textBaseline = "middle";
        //c.textAlign = "center";
        //c.fillText('hello', 30, 100);
        $("#videoplayer_html5_api").attr("hidden", true);
        setTimeout(draw, 10, v, c, w, h);
    }
