    <script>
    var A = document.getElementById("videoplayer_html5_api");
    var B = document.getElementById("audioplayer");



    A.onvolumechange = function() {
        var temp_vol = A.volume;
        B.volume = temp_vol;
    };






    A.addEventListener("play", function() {
        if (A.readyState != 4 || B.readyState != 4) {
            B.pause();
            A.pause();
        } else if (A.readyState == 4 && B.readyState == 4 && A.currentTime > 0 && B.currentTime > 0) {
            B.play();
        } else {
            B.play();
            B.currentTime = 0;
            A.currentTime = 0;
            console.log(B.currentTime - A.currentTime);
        }
    });

    A.addEventListener("pause", function() {
        B.pause();
    });

    A.addEventListener("seeking", function() {
        B.currentTime = A.currentTime;
    });

    A.addEventListener("seeked", function() {
        B.currentTime = A.currentTime;
    });


    A.onwaiting = function() {
        B.pause();
    };

    A.onplaying = function() {
        B.play();
    };

